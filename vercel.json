{
  "headers": [
    {
      "source": "/(.*)\\.(jpg|jpeg|png|webp|avif|gif|svg)$",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-store" }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
